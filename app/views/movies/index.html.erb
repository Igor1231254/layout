<!-- app/views/movies/index.html.erb -->
<!DOCTYPE html>
<html>
<head>
  <title>All Films</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  
</head>
<body>
  <h1>Films</h1>
  <ul>
    <% @movies.each do |movie| %>
      <li><%= movie.title %></li>
    <% end %>
    <%= render 'layouts/header' %>
  </ul>
</body>
</html>
